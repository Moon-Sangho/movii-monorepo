{
  "$schema": "https://turborepo.dev/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@movii/web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "out/**"],
      "env": ["NEXT_PUBLIC_TMDB_API_ACCESS_TOKEN"]
    },
    "lint": { "outputs": [] },
    "lint:fix": { "outputs": [] },
    "typecheck": { "outputs": [] }
  }
}
